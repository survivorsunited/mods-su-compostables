{
	"schemaVersion": 1,
	"id": "${mod_id}",
	"version": "${mod_version}",
	"name": "${mod_name}",
	"description": "${mod_description}",
	"authors": [
		"${mod_author}"
	],
	"contact": {
		"homepage": "${mod_homepage}",
		"sources": "${mod_sources}",
		"issues": "${mod_issues}"
	},
	"license": "${mod_license}",
	"icon": "${mod_icon}",
	"environment": "*",
	"entrypoints": {
		"main": [
			"${main_class}"
		]
	},
	"mixins": [
		"compostables.mixins.json"
	],
	"depends": {
		"fabricloader": ">=${fabric_loader_version}",
		"minecraft": ">=${minecraft_version}",
		"java": ">=${java_version}",
		"fabric-api": "${fabric_api_version}"
	}
}